{
  "name": "tunemeld-frontend",
  "version": "1.0.0",
  "description": "TuneMeld Frontend TypeScript Application",
  "scripts": {
    "dev": "node dev.js",
    "dev:v2": "vite",
    "build": "node build.js",
    "build:v2": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/v2/**/*.{ts,tsx}\" \"src/main-v2.tsx\"",
    "format": "prettier --write \"src/v2/**/*.{ts,tsx}\" \"src/main-v2.tsx\"",
    "format:check": "prettier --check \"src/v2/**/*.{ts,tsx}\" \"src/main-v2.tsx\""
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-classnames": "^0.8.5",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^3.4.13",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.0",
    "vite": "^6.0.7"
  },
  "dependencies": {
    "@react-three/fiber": "^9.4.0",
    "@react-three/postprocessing": "^3.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "postprocessing": "^6.38.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "three": "^0.181.0",
    "tw-animate-css": "^1.4.0"
  }
}
