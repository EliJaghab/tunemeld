{
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "backend/core/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -e ."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/core/wsgi.py"
    }
  ]
}
