{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/simple-gql/(.*)",
      "destination": "/api/simple_gql"
    },
    {
      "source": "/gql/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/api/index"
    }
  ]
}
