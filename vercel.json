{
  "functions": {
    "main.py": {
      "runtime": "@vercel/python@3.12.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/gql/(.*)?",
      "dest": "/main.py"
    },
    {
      "src": "/graphql/(.*)?",
      "dest": "/main.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ]
}
