{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.12"
    },
    "api/test.py": {
      "runtime": "@vercel/python@3.12"
    },
    "api/debug.py": {
      "runtime": "@vercel/python@3.12"
    },
    "api/simple_test.py": {
      "runtime": "@vercel/python@3.12"
    }
  },
  "rewrites": [
    {
      "source": "/gql/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/api/index"
    }
  ]
}
