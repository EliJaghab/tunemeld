{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/fastapi-test/(.*)",
      "destination": "/api/fastapi_test"
    },
    {
      "source": "/fastapi-gql/(.*)",
      "destination": "/api/fastapi_gql"
    },
    {
      "source": "/gql/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/api/index"
    }
  ]
}
